FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Libspotify/libspotify-12.1.64-iOS-universal"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/CocoaLibSpotify" "${PODS_ROOT}/Headers/libspotify"
OTHER_CFLAGS = $(inherited) "-isystem${PODS_ROOT}/Headers" "-isystem${PODS_ROOT}/Headers/CocoaLibSpotify" "-isystem${PODS_ROOT}/Headers/libspotify"
OTHER_LDFLAGS = -ObjC -ObjC -all_load -lstdc++ -framework AVFoundation -framework AudioToolbox -framework CFNetwork -framework CoreAudio -framework Security -framework SystemConfiguration -framework libspotify
PODS_ROOT = ${SRCROOT}/Pods